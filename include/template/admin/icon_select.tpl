<script>
$('#icon-select').click(function(){         
    $('#iconModal').modal({
        backdrop:true,
        keyboard:true,
        show:true
    }); 
    $(".modal-open").css("overflow","hidden");
});
$('.icon').click(function(){
    var obj=$(this);
    $('#icon-preview').removeClass().addClass(obj.children("i").attr("class"));
    $('#icon-name').val(obj.text());
    $('#iconModal').modal('toggle');
});
$("#icon-name").keyup(function(){
    var name = $(this).val();
    $('#icon-preview').attr("class","fa "+name);
})
$("#iconModal").on("hidden.bs.modal",function(){
    $(".modal-open").css("overflow","auto");
})
//图标翻页
$(".pagination li a").click(function(){
    var cls = $(this).attr("class");
    var pnum = $("#iconModal .row").length;
    var pn,tn = 0;
    if(cls){
        tn = parseInt($('.pagination .active a').attr("rel"));
        if(cls=="prev"&&tn-1>0){
            pn = tn-1;
        }else if(cls=="next"&&tn+1<pnum){
            pn = tn+1;
        }else{
            return;
        }
    }else{
        pn = parseInt($(this).attr("rel"));
    }
    var gn = $(".pagination li a[rel="+pn+"]");
    var box = $("#iconModal .row:eq("+(pn-1)+")");
    $(".pagination li").removeClass("active");
    gn.parent("li").addClass("active");
    $("#iconModal .row").addClass("hide");
    box.removeClass("hide");
    $(".icon-type").html(box.attr("type"));
})
</script>